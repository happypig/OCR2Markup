<?xml version="1.0" encoding="UTF-8"?>
<!-- The plugin.dtd file is located in the OXYGEN_INSATALL_DIR/plugins directory -->
<!DOCTYPE plugin SYSTEM "../plugin.dtd">
<plugin
    id="com.oxygenxml.plugin.Conversion"
    name="Conversion"
    description="Conversion plug-in for oXygen"
    version="${project.nosnapshot.version}"
    vendor="Syncro Soft"
    class="DILA.converter.ConversionPlugin"
    classLoaderType="preferReferencedResources">
    <runtime>
        <library name="lib/${project.build.finalName}.jar"/>
    </runtime>
    <extension type="selectionProcessor" 
    class="DILA.converter.ConversionPluginExtension"/>
</plugin>
