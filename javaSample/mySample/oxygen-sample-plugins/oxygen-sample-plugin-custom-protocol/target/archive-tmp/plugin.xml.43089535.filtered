<?xml version="1.0" encoding="UTF-8"?>
<!-- The plugin.dtd file is located in the OXYGEN_INSATALL_DIR/plugins directory -->
<!DOCTYPE plugin SYSTEM "../plugin.dtd">

<plugin
 id="DILA.customprotocol.CustomProtocol"
 name="CustomProtocol"
 description="An example of a custom protocol implementation allowing access to local resources."
 version="1.0"
 vendor="Syncro Soft"
 class="DILA.customprotocol.CustomProtocolPlugin"
 classLoaderType="preferReferencedResources">
 <runtime>
        <library name="lib/oxygen-sample-plugin-custom-protocol-1.0.jar"/>
 </runtime>
 
 <extension type="URLHandler" 
  class="DILA.customprotocol.CustomProtocolURLHandlerExtension"/>
 
 <extension type="URLChooser" 
  class="DILA.customprotocol.CustomProtocolChooserExtension"/>
 
 <extension type="URLChooserToolbar" 
  class="DILA.customprotocol.CustomProtocolChooserExtension"/>
 
</plugin>
